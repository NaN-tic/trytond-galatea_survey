<data>
    <xpath expr="/form/group[@id=&quot;checkboxes&quot;]/field[@name=&quot;active&quot;]" position="after">
        <label name="esale"/>
        <field name="esale"/>
    </xpath>
    <xpath expr="/form/notebook" position="inside">
        <page string="eSale" id="esale"  states="{'invisible': Not(Bool(Eval('esale')))}">
            <label name="slug"/>
            <field name="slug"/>
            <label name="css"/>
            <field name="css"/>
            <label name="login"/>
            <field name="login"/>
            <label name="manager"/>
            <field name="manager"/>
            <separator name="websites" colspan="6"/>
            <field name="websites" colspan="6"/>
            <separator name="esale_description" colspan="6"/>
            <field name="esale_description" colspan="6"/>
            <separator name="esale_response" colspan="6"/>
            <field name="esale_response" colspan="6"/>
            <separator name="esale_notes" colspan="6"/>
            <field name="esale_notes" colspan="6"/>
        </page>
    </xpath>
</data>
